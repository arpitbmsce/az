const array = [2, 5, 4, 5, 3, 1 ,3, 4, 2,  4];
console.log(firstRecurringNumber(array)); O(n2) solution giving 5 as output
console.log(firstRecurringNumber2(array)); O(n) solution giving 2 as solution

const array2 = [100, 2, 101, 1, 3, 200, 4];
console.log(longestContigousSubsequence(array2)) // outputs 4 - length of longest continous sequence (1,2,3,4) O(nlogn) / O(n)
